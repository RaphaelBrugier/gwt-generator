<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.contentPanel {
			border: 5px solid #DCE9F2;
			-moz-border-radius: 10px; 
			-webkit-border-radius: 10px;
			margin : 10px;
			padding : 5px;
		}
	</ui:style>
	<g:DockLayoutPanel unit='PX' height="100%" width="100%">
		<g:north size="25">
			<g:Button ui:field="saveButton">Save</g:Button>
		</g:north>
		<g:center>
			<g:LayoutPanel ui:field="contentPanel" addStyleNames ="{style.contentPanel}"/>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 